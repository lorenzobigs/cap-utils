{
    "name": "demo",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
      "@sap/cds": "^7",
      "express": "^4",
      "@sap/xssec": "^3",
      "@sap/cds-hana": "^2"
    },
    "devDependencies": {
      "@cap-js/sqlite": "^1",
      "@sap/cds-dk": "^7"
    },
    "scripts": {
      "start": "cds-serve"
    },
    "cds": {
      "requires": {
        "nw": {
          "kind": "odata",
          "model": "srv/external/nw",
          "credentials" : {
            "[local]" :{
              "url" : "https://services.odata.org/v4/northwind/northwind.svc/"
            },
            "[production]" :{
              "destination" : "nw",
              "forwardAuthToken": true
            }
          }
        },
        "[production]": {
          "auth": "xsuaa",
          "db": "hana",
          "destinations": true,
          "html5-repo": true
        },
        "[local]" : {
          "auth": {
              "kind": "mocked",
              "users": {
                "basic_user": {
                  "password": "pw",
                  "roles": ["Basic_Scope"]
                }
              }
          }
        }
      }
    }
  }
  